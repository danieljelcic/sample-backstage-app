{
	"version": "2.0.0",
	"tasks": [
        {
            "label": "start-frontend",
            "type": "npm",
            "script": "start",
            "path": "${workspaceFolder}",
            "isBackground": true,
            "problemMatcher": {
                "owner": "webpack",
                "pattern": {
                    "regexp": "."
                },
                "background": {
                    "activeOnStart": true,
                    "beginsPattern": "Starting the development server",
                    "endsPattern": "webpack compiled successfully"
                }
            }
        }
    ]
}